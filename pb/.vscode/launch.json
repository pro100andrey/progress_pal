{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Seeder Push (debug)",
            "request": "launch",
            "program": "seeder/bin/seeder.dart",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "push",
                "--env=.env",
                "--truncate",
                "--verbose"
            ]
        },
        {
            "name": "Seeder Pull (debug)",
            "request": "launch",
            "program": "seeder/bin/seeder.dart",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "pull",
                "--env=.env",
                "--verbose"
            ]
        },
        {
            "name": "Seeder Init (debug)",
            "request": "launch",
            "program": "seeder/bin/seeder.dart",
            "type": "dart",
            "flutterMode": "debug",
            "console": "terminal",
            "presentation": {
                "order": 1,
                "focus": true,
                "hidden": false,
                "group": "Seeder"
            },
            "args": [
                "init",
                "--verbose",
                "--data-dir=./seeder_data"
            ]
        },
    ]
}